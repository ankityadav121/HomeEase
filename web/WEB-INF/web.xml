<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>registrationpage</servlet-name>
        <servlet-class>registeruser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginpage</servlet-name>
        <servlet-class>Loginworker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Loginworker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerworker</servlet-name>
        <servlet-class>registerworker</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registeruser</servlet-name>
        <servlet-class>registeruser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginuser</servlet-name>
        <servlet-class>loginuser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUploadDBServlet</servlet-name>
        <servlet-class>FileUploadDBServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>uploadImage</servlet-name>
        <servlet-class>uploadImage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registrationpage</servlet-name>
        <url-pattern>/registrationpage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginpage</servlet-name>
        <url-pattern>/loginpage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/Loginworker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerworker</servlet-name>
        <url-pattern>/registerworker</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registeruser</servlet-name>
        <url-pattern>/registeruser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginuser</servlet-name>
        <url-pattern>/loginuser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUploadDBServlet</servlet-name>
        <url-pattern>/FileUploadDBServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>uploadImage</servlet-name>
        <url-pattern>/uploadImage</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
